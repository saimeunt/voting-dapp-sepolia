{
  "name": "voting-dapp-sepolia",
  "version": "1.0.0",
  "description": "Voting dApp",
  "author": "saimeunt",
  "license": "ISC",
  "scripts": {
    "lint": "next lint",
    "prettier": "prettier --write \"{app,components,contracts,lib}/**/*.{ts,tsx,sol}\"",
    "tsc": "tsc --noEmit",
    "dev": "next dev",
    "prebuild": "yarn hardhat:compile",
    "build": "next build",
    "hardhat:node": "hardhat node",
    "hardhat:compile": "hardhat compile",
    "hardhat:deploy:localhost": "hardhat run scripts/deploy.ts --network localhost",
    "hardhat:deploy:sepolia": "hardhat run scripts/deploy.ts --network sepolia",
    "hardhat:test": "hardhat test test/test.ts",
    "hardhat:coverage": "hardhat coverage",
    "hardhat:verify": "hardhat verify --network sepolia 0xeb9BC6BCaD612Fd5cAF1cA8406D746cc02E675d9"
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "trailingComma": "all",
    "plugins": [
      "prettier-plugin-solidity",
      "prettier-plugin-tailwindcss"
    ],
    "overrides": [
      {
        "files": "*.sol",
        "options": {
          "printWidth": 100,
          "tabWidth": 2
        }
      }
    ]
  },
  "dependencies": {
    "@headlessui/react": "^1.7.14",
    "@heroicons/react": "^2.0.18",
    "@openzeppelin/contracts": "^4.9.0",
    "@web3modal/ethereum": "^2.4.1",
    "@web3modal/react": "^2.4.1",
    "classnames": "^2.3.2",
    "dotenv": "^16.0.3",
    "ethers": "^5.7.2",
    "lodash": "^4.17.21",
    "next": "^13.4.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "usehooks-ts": "^2.9.1",
    "viem": "^0.3.37",
    "wagmi": "^1.0.8"
  },
  "devDependencies": {
    "@ethersproject/abi": "^5.7.0",
    "@ethersproject/providers": "^5.7.2",
    "@nomicfoundation/hardhat-chai-matchers": "^1.0.6",
    "@nomicfoundation/hardhat-network-helpers": "^1.0.8",
    "@nomicfoundation/hardhat-toolbox": "^2.0.2",
    "@nomicfoundation/hardhat-verify": "^1.0.0",
    "@nomiclabs/hardhat-ethers": "^2.2.3",
    "@nomiclabs/hardhat-etherscan": "^3.1.7",
    "@nomiclabs/hardhat-waffle": "^2.0.6",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.3",
    "@typechain/ethers-v5": "^10.2.1",
    "@typechain/hardhat": "^6.1.6",
    "@types/chai": "^4.3.5",
    "@types/lodash": "^4.14.195",
    "@types/mocha": "^10.0.1",
    "@types/node": "^20.2.5",
    "@types/react": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^5.59.7",
    "@typescript-eslint/parser": "^5.59.7",
    "autoprefixer": "^10.4.14",
    "chai": "^4.3.7",
    "eslint": "^8.41.0",
    "eslint-config-next": "^13.4.4",
    "eslint-config-prettier": "^8.8.0",
    "hardhat": "^2.14.0",
    "hardhat-gas-reporter": "^1.0.9",
    "postcss": "^8.4.24",
    "prettier": "^2.8.8",
    "prettier-plugin-solidity": "^1.1.3",
    "prettier-plugin-tailwindcss": "^0.3.0",
    "solidity-coverage": "^0.8.2",
    "tailwindcss": "^3.3.2",
    "ts-node": "^10.9.1",
    "typechain": "^8.2.0",
    "typescript": "^5.0.4"
  }
}
